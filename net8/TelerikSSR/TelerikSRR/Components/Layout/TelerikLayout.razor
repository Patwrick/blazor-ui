@rendermode RenderMode.InteractiveServer

<TelerikRootComponent>

    <div class="page">
        <main>
            <div class="top-row px-4">
                <InteractiveAppBar />
            </div>

            <article class="content px-4">
                @ChildContent
            </article>
        </main>
    </div>

</TelerikRootComponent>

@code {
    [Parameter]
    public RenderFragment ChildContent { get; set; }
}